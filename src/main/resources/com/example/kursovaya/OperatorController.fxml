<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kursovaya.OperatorController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="601.0" style="-fx-background-color: #D2B48C;">
