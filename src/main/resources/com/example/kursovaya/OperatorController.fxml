               </columns>
            </TableView>
            <Button fx:id="BackButton" layoutX="14.0" layoutY="354.0" mnemonicParsing="false" prefHeight="19.0" prefWidth="62.0" style="-fx-background-color: orange;" text="Назад" textFill="WHITE">
               <font>
                  <Font name="Palatino Linotype" size="12.0" />
               </font>
            </Button>
            <Button fx:id="deleteButton" layoutX="136.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="71.0" style="-fx-background-color: orange;" text="Удалить" textFill="WHITE">
               <font>
                  <Font name="Palatino Linotype" size="12.0" />
               </font>
            </Button>
            <TextField fx:id="searchfield" layoutX="316.0" layoutY="14.0" promptText="Искать..." />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
